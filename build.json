{
  "vite": true,
  "plugins": [
    [
      "build-plugin-ignore-style",
      {
        "libraryName": "antd"
      }
    ]
  ],
  "externals": {
    "react": "React",
    "react-dom": "ReactDOM",
    "@ant-design/charts": "charts"
  },
  "proxy": {
    "/api": {
      "enable": true,
      "target": "http://127.0.0.1:7001"
    }
  },
  "mock": {
    "exclude": ["**"]
  }
}
